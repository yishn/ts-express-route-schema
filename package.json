{
  "name": "ts-route-schema",
  "version": "1.0.0",
  "description": "Strictly typed, isomorphic routes.",
  "main": "./dist/main.js",
  "scripts": {
    "test": "tap --test-regex=test\\/.*\\.test\\.ts$ --reporter=base",
    "format": "prettier --write .",
    "build": "prettier --check . && tsc --project ./tsconfig.build.json",
    "docs": "typedoc --out ./docs --excludePrivate --stripInternal ./src/main.ts && node --eval \"require('fs').writeFileSync('./docs/.nojekyll')\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yishn/typed-express-router.git"
  },
  "keywords": [
    "express",
    "server",
    "browser",
    "isomorphic",
    "routes",
    "fetch",
    "typescript",
    "ts",
    "types",
    "api",
    "documentation",
    "docs"
  ],
  "author": "Yichuan Shen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yishn/typed-express-router/issues"
  },
  "homepage": "https://github.com/yishn/typed-express-router#readme",
  "prettier": {
    "arrowParens": "avoid",
    "proseWrap": "always",
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/qs": "^6.9.5",
    "fetch-ponyfill": "^7.0.0",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.3",
    "@types/tap": "^14.10.1",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "prettier": "^2.1.2",
    "tap": "^14.10.8",
    "ts-expect": "^1.1.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  }
}
